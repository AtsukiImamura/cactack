{
  "compilerOptions": {
    // "noImplicitAny": true,
    "module": "es6",
    "target": "es2015",
    // "jsx": "react",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictFunctionTypes": false,
    "experimentalDecorators": true,
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      //   "config/*": ["src/model/*"],
      //   "env/*": ["src/env/*"],
      //   "model/*": ["src/model/*"],
      //   "repository/*": ["src/repository/*"],
      //   "resources/*": ["src/resources/*"],
      //   "service/*": ["src/service/*"],
      //   "util/*": ["src/util/*"],
      //   "view/*": ["src/view/*"],
      "@": ["src"],
      "@*": ["src/*"]
    },
    "typeRoots": ["src/declaration", "node_modules/@types"]
  },
  "exclude": ["node_modules/*"],
  "include": ["src/**/*.ts", "src/**/*.vue"]
}
